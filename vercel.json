{
   "version": 2,
   "builds": [
      {
         "src": "./Backend/Server.js",
         "use": "@vercel/node"
      },
      {
         "src": "./frontend/package.json",
         "use": "@vercel/static-build"
      }
   ],
   "routes": [
      {
         "src": "/(.*)",
         "dest": "./Backend/Server.js"
      },
      {
         "src": "/(.*)",
         "dest": "/frontend/build/index.html"
      }
   ]
}